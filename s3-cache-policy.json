{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "S3CacheAccess",
      "Effect": "Allow",
      "Action": [
        "s3:GetObject",
        "s3:PutObject",
        "s3:DeleteObject",
        "s3:ListBucket",
        "s3:HeadObject"
      ],
      "Resource": [
        "arn:aws:s3:::rag-cache-bucket",
        "arn:aws:s3:::rag-cache-bucket/*"
      ]
    }
  ]
}